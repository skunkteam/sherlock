{
    "name": "@skunkteam/sherlock",
    "version": "5.0.0",
    "license": "MIT",
    "repository": "github:skunkteam/sherlock",
    "scripts": {
        "nx": "nx",
        "start": "nx serve",
        "build": "nx run-many --all --target build --configuration production",
        "test": "nx run-many --all --target test",
        "lint": "nx workspace-lint && nx run-many --all --target lint && nx format:check",
        "e2e": "nx e2e",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "update": "nx migrate latest",
        "workspace-generator": "nx workspace-generator",
        "dep-graph": "nx dep-graph",
        "help": "nx help",
        "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
    },
    "standard-version": {
        "bumpFiles": [
            "package.json",
            "libs/sherlock/package.json",
            "libs/sherlock-utils/package.json",
            "libs/ngx-sherlock/package.json",
            "libs/ngx-sherfire/package.json",
            "package-lock.json"
        ],
        "scripts": {
            "postchangelog": "npx prettier -w CHANGELOG.md"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^11.0.0",
        "@angular/common": "^11.0.0",
        "@angular/compiler": "^11.0.0",
        "@angular/core": "^11.0.0",
        "@angular/forms": "^11.0.0",
        "@angular/platform-browser": "^11.0.0",
        "@angular/platform-browser-dynamic": "^11.0.0",
        "@angular/router": "^11.0.0",
        "firebase": "^8.2.4",
        "rxjs": "^6.0.0",
        "tslib": "^2.0.0",
        "zone.js": "^0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1100.1",
        "@angular-eslint/eslint-plugin": "^1.1.0",
        "@angular-eslint/eslint-plugin-template": "^1.1.0",
        "@angular-eslint/template-parser": "^1.1.0",
        "@angular/compiler-cli": "^11.0.0",
        "@angular/language-service": "^11.0.0",
        "@nrwl/angular": "^11.1.5",
        "@nrwl/cli": "11.1.5",
        "@nrwl/eslint-plugin-nx": "11.1.5",
        "@nrwl/jest": "11.1.5",
        "@nrwl/node": "^11.1.5",
        "@nrwl/tao": "11.1.5",
        "@nrwl/workspace": "11.1.5",
        "@types/jest": "26.0.8",
        "@types/node": "12.12.38",
        "@typescript-eslint/eslint-plugin": "4.3.0",
        "@typescript-eslint/parser": "4.3.0",
        "codelyzer": "^6.0.0",
        "dotenv": "6.2.0",
        "eslint": "7.10.0",
        "eslint-config-prettier": "6.0.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^31.0.8",
        "eslint-plugin-prefer-arrow": "^1.2.2",
        "immutable": "^4.0.0-rc.12",
