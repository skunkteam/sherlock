        "jest-preset-angular": "8.3.1",
        "ng-packagr": "^11.0.0",
        "prettier": "2.2.1",
        "standard-version": "^9.1.0",
        "ts-jest": "26.4.0",
        "ts-node": "~9.1.1",
        "tslint": "~6.1.0",
        "typescript": "~4.0.3"
    }
